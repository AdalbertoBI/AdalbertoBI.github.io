{
  "name": "whatintegra-local-auth-server",
  "version": "1.0.0",
  "description": "Servidor local para autenticação do site WhatIntegra Pages",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run auth\" \"npm run whatsapp\"",
    "auth": "node server.js",
    "whatsapp": "node whatsapp-server.js",
    "create:user": "node create-user.js",
    "dev": "concurrently --names \"AUTH,WHATSAPP\" --prefix-colors \"blue,green\" \"npm run auth\" \"npm run whatsapp\"",
    "cluster": "node ../cluster.js",
    "production": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "puppeteer": "^24.22.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.8.1",
    "whatsapp-web.js": "^1.34.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.7",
    "selfsigned": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "whatsapp",
    "web",
    "authentication",
    "express",
    "nodejs"
  ],
  "author": "WhatIntegra Team",
  "license": "MIT"
}
